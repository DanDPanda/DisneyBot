{
  "name": "disneybot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --coverage",
    "start": "node --experimental-modules ./src/index.mjs",
    "unit": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanDPanda/DisneyBot.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DanDPanda/DisneyBot/issues"
  },
  "homepage": "https://github.com/DanDPanda/DisneyBot#readme",
  "dependencies": {
    "chance": "^1.1.3",
    "discord.js": "^11.5.1",
    "dotenv": "^8.2.0",
    "node-cron": "^2.0.3"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{mjs,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "src/command-list.mjs",
      "src/index.mjs"
    ]
  },
  "devDependencies": {
    "jest": "^24.9.0"
  }
}
